{
  "name": "@amehmeto/tied-siren-blocking-overlay",
  "version": "0.1.0",
  "description": "An Expo module that displays a fullscreen blocking overlay on Android for TiedSiren",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "lint:fix": "expo-module lint --fix",
    "test": "expo-module test",
    "prepare": "expo-module prepare && husky",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix --"
    ],
    "android/**/*.kt": [
      "ktlint --format"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "react-native",
    "expo",
    "tied-siren",
    "blocking-overlay",
    "android"
  ],
  "repository": "https://github.com/amehmeto/tied-siren-blocking-overlay",
  "bugs": {
    "url": "https://github.com/amehmeto/tied-siren-blocking-overlay/issues"
  },
  "author": "amehmeto",
  "license": "MIT",
  "homepage": "https://github.com/amehmeto/tied-siren-blocking-overlay#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "expo-module-scripts": "^4.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
